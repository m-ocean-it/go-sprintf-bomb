package p

import (
	"errors"
	"strconv"
)

func foo() {
	i64 := int64(2)
	_ = "High " + strconv.FormatInt(i64, 10) + "!" // want "Sprintf could be optimized away"

	i32 := int32(2)
	_ = "High " + strconv.FormatInt(int64(i32), 10) + "!" // want "Sprintf could be optimized away"

	i16 := int16(2)
	_ = "High " + strconv.FormatInt(int64(i16), 10) + "!" // want "Sprintf could be optimized away"

	i8 := int8(2)
	_ = "High " + strconv.FormatInt(int64(i8), 10) + "!" // want "Sprintf could be optimized away"

	i := 2
	_ = strconv.Itoa(i) + " is int" // want "Sprintf could be optimized away"
	_ = strconv.Itoa(i) // want "Sprintf could be optimized away"

	_ = "a" + ", " + "b" + ", " + "c" // want "Sprintf could be optimized away"

	_ = "John" + " is " + strconv.Itoa(3) + " years old. Pi is " + strconv.FormatFloat(3.14, 'f', -1, 64) // want "Sprintf could be optimized away"

	f32 := float32(3.14)
	_ = "Pi is " + strconv.FormatFloat(float64(f32), 'f', -1, 64) // want "Sprintf could be optimized away"

	cs := customStringer{}
	_ = "This is " + cs.String() // want "Sprintf could be optimized away"

	err := errors.New("some error")
	_ = "this is an error: " + err.Error() // want "Sprintf could be optimized away"

	u := uint(10)
	_ = ":" + strconv.FormatUint(uint64(u), 10) // want "Sprintf could be optimized away"

	u64 := uint64(5000)
	_ = ":" + strconv.FormatUint(u64, 10) // want "Sprintf could be optimized away"

	u32 := uint32(190)
	_ = ":" + strconv.FormatUint(uint64(u32), 10) // want "Sprintf could be optimized away"

	u16 := uint16(5)
	_ = ":" + strconv.FormatUint(uint64(u16), 10) // want "Sprintf could be optimized away"

	u8 := uint8(150)
	_ = ":" + strconv.FormatUint(uint64(u8), 10) // want "Sprintf could be optimized away"

	strerError := stringerError("hello world")
	_ = strerError.Error() // want "Sprintf could be optimized away"

	strError := stringError("hello world")
	_ = strError.Error() // want "Sprintf could be optimized away"

	wrStr := wrappedString("hello world")
	_ = "wrapped string: " + string(wrStr) // want "Sprintf could be optimized away"

	wrInt := wrappedInt(-2)
	_ = "wrapped int: " + strconv.Itoa(int(wrInt)) + "!" // want "Sprintf could be optimized away"

	wrU32 := wrappedUint32(3)
	_ = "wrapped uint32: " + strconv.FormatUint(uint64(wrU32), 10) // want "Sprintf could be optimized away"
}

type customStringer struct{}

func (c customStringer) String() string {
	return "Hello from custom stringer!"
}

type stringerError string

func (s stringerError) String() string {
	return "String()"
}
func (s stringerError) Error() string {
	return "Error()"
}

type stringError string

func (s stringError) Error() string {
	return "Error()"
}

type wrappedString string

type wrappedInt int

type wrappedUint32 uint32
